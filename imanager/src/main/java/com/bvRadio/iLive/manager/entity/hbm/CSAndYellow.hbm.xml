<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.manager.entity">
	<class name="CSAndYellow" table="ilive_checkhealth_task">
		<meta attribute="sync-DAO">false</meta>
		<id name="taskId" type="java.lang.String">
			<column name="task_id" />
			<generator class="assigned" />
		</id>

		<property name="roomId">
			<column name="room_id" />
		</property>

		<property name="eventId">
			<column name="live_event_id" />
		</property>

		<property name="picUrl">
			<column name="picurl" />
		</property>

		<property name="replyMsg">
			<column name="replymsg" />
		</property>

		<property name="monitorLevel">
			<column name="monitor_level" />
		</property>

		<property name="checkTime">
			<column name="check_time" />
		</property>

	</class>
</hibernate-mapping>
