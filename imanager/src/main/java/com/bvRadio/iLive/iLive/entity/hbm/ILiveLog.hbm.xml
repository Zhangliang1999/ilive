<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveLog" table="ilive_log">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="event" type="java.lang.String">
			<column name="event" />
		</property>
		<property name="createTime" type="java.sql.Timestamp">
			<column name="create_time"/>
		</property>
		<property name="status" type="java.lang.Integer">
			<column name="status" />
		</property>
		<property name="roomId" type="java.lang.Integer">
			<column name="room_id" />
		</property>
			<property name="liveId" type="java.lang.Integer">
			<column name="live_id" />
		</property>
	</class>
</hibernate-mapping>
