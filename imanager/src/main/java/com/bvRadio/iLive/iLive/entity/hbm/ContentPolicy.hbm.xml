<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ContentPolicy" table="content_policy">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="enterpriseId" type="java.lang.Integer">
			<column name="enterprise_id" />
		</property>
		<property name="policy" type="java.lang.Integer">
			<column name="policy" />
		</property>

		<property name="num" type="java.lang.Integer">
			<column name="num" />
		</property>

		<property name="radios" type="java.lang.Integer">
			<column name="radios" />
		</property>

		<property name="shows" type="java.lang.Integer">
			<column name="shows" />
		</property>

		<property name="title2" type="java.lang.String">
			<column name="title2" />
		</property>

		<property name="title4" type="java.lang.String">
			<column name="title4" />
		</property>

		<property name="link" type="java.lang.String">
			<column name="link" />
		</property>

		<property name="linkName" type="java.lang.String">
			<column name="linkName" />
		</property>

		<property name="imgurl" type="java.lang.String">
			<column name="imgurl" />
		</property>

	</class>
</hibernate-mapping>