<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveRoomStatics" table="ilive_room_timestatics">
		<meta attribute="sync-DAO">false</meta>
		
		<id name="liveEventId" type="java.lang.Long">
			<column name="liveEvent_id" />
			<generator class="assigned" />
		</id>
		
		
		<property name="showNum" type="java.lang.Long">
			<column name="show_num" />
		</property>

	</class>
</hibernate-mapping>
