<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="BBSDiyformName" table="bbs_diyform_name">
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write" />
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="diyformId" type="java.lang.Integer">
			<column name="diyform_id" />
		</property>
		<property name="diyformName" type="java.lang.String">
			<column name="diyform_name" />
		</property>
		<property name="updateMark" type="java.lang.Integer">
			<column name="update_mark" />
		</property>
		<property name="createTime">
			<column name="create_time" />
		</property>

	</class>
</hibernate-mapping>
