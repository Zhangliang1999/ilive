<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="TopicTemplate" table="topic_template">

		<meta attribute="sync-DAO">false</meta>

		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>

		<property name="name">
			<column name="name"></column>
		</property>

		<property name="backgroundColor">
			<column name="background_color"></column>
		</property>

		<property name="fontColor">
			<column name="font_color"></column>
		</property>

		<property name="fontType">
			<column name="font_type"></column>
		</property>

		<property name="enterpriseId">
			<column name="enterprise_id"></column>
		</property>

	</class>
</hibernate-mapping>