<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveViewSetRecord" table="ilive_viewset_record">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>
		
		<property name="viewAuthorized" type="java.lang.Integer">
			<column name="view_authorized"></column>
		</property>
		
		<property name="welcome" type="java.lang.String">
			<column name="welcome"></column>
		</property>
		
		<property name="viewPassword" type="java.lang.String">
			<column name="view_password"></column>
		</property>
		
		<property name="payAmount" type="java.lang.Double">
			<column name="pay_amount"></column>
		</property>
		
		<property name="userName" type="java.lang.String">
			<column name="user_name"></column>
		</property>
		
		<property name="updateTime" type="java.sql.Timestamp">
			<column name="update_time"></column>
		</property>
		
		<property name="roomId" type="java.lang.Integer">
			<column name="room_id"></column>
		</property>
		
	</class>
</hibernate-mapping>