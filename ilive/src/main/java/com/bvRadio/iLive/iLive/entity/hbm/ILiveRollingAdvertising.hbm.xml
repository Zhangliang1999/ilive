<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveRollingAdvertising" table="ilive_rolling_advertising">
		<meta attribute="sync-DAO">false</meta>
		<id name="roomId" type="java.lang.Integer">
			<column name="room_id" />
			<generator class="assigned" />
		</id>
		
		<property name="content" type="java.lang.String">
			<column name="content"/>
		</property>
		
		<property name="startType" type="java.lang.Integer">
			<column name="start_type"/>
		</property>
		
	</class>
</hibernate-mapping>
