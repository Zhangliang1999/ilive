<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveUserPlayRewards" table="ilive_play_rewards_user">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="roomId" type="java.lang.Integer">
			<column name="room_id"/>
		</property>
		<property name="evenId" type="java.lang.Long">
			<column name="even_id"/>
		</property>
		<property name="roomTitle" type="java.lang.String">
			<column name="room_title"/>
		</property>
		<property name="userId" type="java.lang.Long">
			<column name="user_id"/>
		</property>
		<property name="userName" type="java.lang.String">
			<column name="user_name"/>
		</property>
		<property name="playRewardsTime" type="java.sql.Timestamp">
			<column name="play_rewards_time"/>
		</property>
		<property name="playRewardsAmount" type="java.lang.String">
			<column name="play_rewards_amount"/>
		</property>
	</class>
</hibernate-mapping>
