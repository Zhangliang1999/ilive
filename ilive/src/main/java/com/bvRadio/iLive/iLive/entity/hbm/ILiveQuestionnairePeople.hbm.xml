<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveQuestionnairePeople" table="ilive_questionnaire_people">

		<meta attribute="sync-DAO">false</meta>

		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>

		<property name="questionnaireId">
			<column name="questionnaire_id"></column>
		</property>

		<property name="questionnaireProblemId">
			<column name="questionnaire_problem_id"></column>
		</property>

		<property name="questionnaireOptionId">
			<column name="questionnaire_option_id"></column>
		</property>
		
		<property name="answer">
			<column name="answer"></column>
		</property>
		
		<property name="userId">
			<column name="user_id"></column>
		</property>

		<property name="mobile">
			<column name="mobile"></column>
		</property>
		<property name="userName">
			<column name="user_name"></column>
		</property>
		<property name="IdCard">
			<column name="id_card"></column>
		</property>

		<property name="createTime">
			<column name="create_time"></column>
		</property>
	</class>
</hibernate-mapping>