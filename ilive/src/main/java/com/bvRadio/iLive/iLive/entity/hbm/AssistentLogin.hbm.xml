<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="AssistentLogin" table="ilive_assistent_login">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="userName" type="java.lang.String">
			<column name="user_name" />
		</property>
		<property name="loginTime" type="java.sql.Timestamp">
			<column name="login_time"/>
		</property>
		<property name="ip" type="java.lang.String">
			<column name="ip" />
		</property>
		<property name="roomId" type="java.lang.Integer">
			<column name="room_id"></column>
		</property>
		<property name="invitationCode" type="java.lang.String">
			<column name="invitation_code"/>
		</property>
	</class>
</hibernate-mapping>
