<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveSubAccountManager" table="ilive_subAccount_manager">
	<meta attribute="sync-DAO">false</meta>
	<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="enterpriseId" >
			<column name="enterprise_id" />
		</property>
		<property name="user" >
			<column name="user" />
		</property>
		<property name="name" >
			<column name="name" />
		</property>
		<property name="contactPhone" >
			<column name="contact_phone" />
		</property>
		<property name="creatTime" >
			<column name="creatTime" />
		</property>
		<property name="reason" >
			<column name="reason" />
		</property>
		<property name="process" >
			<column name="process" />
		</property>
		<property name="result" >
			<column name="result" />
		</property>
		<property name="refuse" >
			<column name="refuse" />
		</property>
	</class>
</hibernate-mapping>
