<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveRoomRegister" table="ilive_room_register">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="java.lang.Long">
			<column name="id"></column>
			<generator class="assigned"></generator>
		</id>
		<property name="userId" type="java.lang.String">
			<column name="user_id"></column>
		</property>
		<property name="roomId" type="java.lang.Integer">
			<column name="room_id"></column>
		</property>
		<property name="fileId" type="java.lang.Long">
			<column name="file_id"></column>
		</property>
		<property name="liveEventId" type="java.lang.Long">
			<column name="live_event_id"></column>
		</property>
		<property name="state" type="java.lang.Integer">
			<column name="state"></column>
		</property>
		<property name="createTime" type="java.sql.Timestamp">
			<column name="create_time"></column>
		</property>
		<property name="isDel" type="java.lang.Integer">
			<column name="is_del"></column>
		</property>
	</class>
</hibernate-mapping>
