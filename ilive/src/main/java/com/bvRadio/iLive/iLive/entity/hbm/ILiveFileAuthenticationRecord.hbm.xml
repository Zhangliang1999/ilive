<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveFileAuthenticationRecord" table="ilive_file_auth_record">
		<meta attribute="sync-DAO">false</meta>
		<id name="recordId" type="java.lang.Long">
			<column name="record_id" />
			<generator class="assigned" />
		</id>
		<property name="userId" type="java.lang.Long">
			<column name="user_id" />
		</property>

		<property name="fileId" type="java.lang.Long">
			<column name="file_id" />
		</property>

		<property name="authType" type="java.lang.Integer">
			<column name="auth_type" />
		</property>

		<property name="authPassTime" type="java.sql.Timestamp">
			<column name="auth_pass_time" />
		</property>

		<property name="deleteStatus" type="java.lang.Integer">
			<column name="delete_status" />
		</property>

	</class>
</hibernate-mapping>
