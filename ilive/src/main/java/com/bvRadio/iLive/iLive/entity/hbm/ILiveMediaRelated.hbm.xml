<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveMediaFileRelated" table="ilive_media_file_related">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="java.lang.String">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="mainFileId" type="java.lang.Long">
			<column name="main_file_id" />
		</property>
		<property name="relatedFileId" type="java.lang.Long">
			<column name="related_file_id" />
		</property>
		<property name="orderNum" type="java.lang.Double">
			<column name="order_num" />
		</property>
		<property name="createTime" type="java.sql.Timestamp">
			<column name="create_time" />
		</property>

	</class>
</hibernate-mapping>
