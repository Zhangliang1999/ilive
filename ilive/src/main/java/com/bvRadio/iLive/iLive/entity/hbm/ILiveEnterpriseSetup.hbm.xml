<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveEnterpriseSetup" table="ilive_enterprise_setup">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="enterpriseId" column="enterprise_id" type="java.lang.Integer"></property>
		<property name="isImg" column="is_img" type="java.lang.Integer"></property>
		<property name="slogan" column="slogan" type="java.lang.String"></property>
		<property name="prompt" column="prompt" type="java.lang.String"></property>
		<property name="advertisementImg" column="advertisement_img" type="java.lang.String"></property>
		
	</class>
</hibernate-mapping>