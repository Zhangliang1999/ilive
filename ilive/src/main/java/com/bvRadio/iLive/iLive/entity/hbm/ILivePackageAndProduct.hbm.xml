<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILivePackageAndProduct" table="ilive_package_and_product">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="LHProductId" >
			<column name="LHProduct_id" />
		</property>
		<property name="LHpackageId" >
			<column name="LHpackage_id" />
		</property>
		<property name="plapackageId" >
			<column name="plapackage_id" />
		</property>
		<property name="ProductId" >
			<column name="product_d" />
		</property>
		<property name="plaProductId" >
			<column name="plaProduct_id" />
		</property>
		<property name="price" >
			<column name="price" />
		</property>
	</class>
</hibernate-mapping>
