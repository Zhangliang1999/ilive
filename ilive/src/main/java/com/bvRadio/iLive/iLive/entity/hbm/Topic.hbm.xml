<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="Topic" table="topic">

		<meta attribute="sync-DAO">false</meta>

		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>

		<property name="templateId">
			<column name="template_id"></column>
		</property>

		<property name="userId">
			<column name="user_id"></column>
		</property>

		<property name="name">
			<column name="name"></column>
		</property>

		<property name="descript">
			<column name="descript"></column>
		</property>

		<property name="logo">
			<column name="logo"></column>
		</property>

		<property name="banner">
			<column name="banner"></column>
		</property>

		<property name="isPublish">
			<column name="is_publish"></column>
		</property>

		<property name="isPut">
			<column name="is_put"></column>
		</property>

		<property name="enterpriseId">
			<column name="enterprise_id"></column>
		</property>

		<property name="createTime">
			<column name="create_time"></column>
		</property>

		<property name="domain">
			<column name="domain"></column>
		</property>

		<property name="userName">
			<column name="user_name"></column>
		</property>

	</class>
</hibernate-mapping>