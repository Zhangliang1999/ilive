<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveGuests" table="ilive_guests">
		<meta attribute="sync-DAO">false</meta>
		<id name="roomId" type="java.lang.Integer">
			<column name="room_id" />
			<generator class="assigned" />
		</id>
		<property name="guestsName" type="java.lang.String">
			<column name="guests_name"/>
		</property>
		<property name="guestsLabel" type="java.lang.String">
			<column name="guests_label"/>
		</property>
	</class>
</hibernate-mapping>
