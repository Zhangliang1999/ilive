<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="Equipment" table="equipment">

		<meta attribute="sync-DAO">false</meta>

		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>

		<property name="name">
			<column name="name"></column>
		</property>

		<property name="img">
			<column name="img"></column>
		</property>

		<property name="descript">
			<column name="descript"></column>
		</property>

		<property name="price">
			<column name="price"></column>
		</property>

		<property name="rentOrSell">
			<column name="rentorsell"></column>
		</property>

		<property name="rentPrice">
			<column name="rent_price"></column>
		</property>

		<property name="rentUnit">
			<column name="rent_unit"></column>
		</property>

		<property name="rentInventory">
			<column name="rent_inventory"></column>
		</property>

		<property name="sellInventory">
			<column name="sell_inventory"></column>
		</property>

		<property name="createTime">
			<column name="create_time"></column>
		</property>

		<property name="updateTime">
			<column name="update_time"></column>
		</property>

		<property name="userId">
			<column name="user_id"></column>
		</property>

		<property name="isShangjia">
			<column name="is_shangjia"></column>
		</property>

		<property name="number">
			<column name="number"></column>
		</property>

	</class>
</hibernate-mapping>