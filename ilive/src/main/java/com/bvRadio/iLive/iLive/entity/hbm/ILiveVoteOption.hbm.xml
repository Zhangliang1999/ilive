<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveVoteOption" table="ilive_vote_option">

		<meta attribute="sync-DAO">false</meta>

		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>

		<property name="voteId">
			<column name="vote_id"></column>
		</property>

		<property name="voteProblemId">
			<column name="vote_problem_id"></column>
		</property>

		<property name="content">
			<column name="content"></column>
		</property>

		<property name="contentImg">
			<column name="content_img"></column>
		</property>

		<property name="createTime">
			<column name="create_time"></column>
		</property>

		<property name="updateTime">
			<column name="update_time"></column>
		</property>

	</class>
</hibernate-mapping>