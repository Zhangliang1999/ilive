<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveFCode" table="ilive_fcode">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="codeName" type="java.lang.String">
			<column name="code_name" />
		</property>

		<property name="code" type="java.lang.String">
			<column name="code" />
		</property>

		<property name="startTime" type="java.sql.Timestamp">
			<column name="start_time" />
		</property>
		<property name="endTime" type="java.sql.Timestamp">
			<column name="end_time" />
		</property>
		<property name="createTime" type="java.sql.Timestamp">
			<column name="create_time" />
		</property>
		<property name="bindUserId" type="java.lang.Long">
			<column name="bind_user_id" />
		</property>
		<property name="bindTime" type="java.sql.Timestamp">
			<column name="bind_time" />
		</property>
		
		<property name="liveRoomId" type="java.lang.Integer">
			<column name="live_roomId" />
		</property>
		<property name="fileId" type="java.lang.Long">
			<column name="file_id" />
		</property>
		
		<property name="status" type="java.lang.Integer">
			<column name="status" />
		</property>
		<property name="bindUserName" type="java.lang.String">
			<column name="bind_userName" />
		</property>
		<property name="bingNailName" type="java.lang.String">
			<column name="bing_nailName" />
		</property>
		<property name="outTime" type="java.sql.Timestamp">
			<column name="out_time" />
		</property>
	</class>
</hibernate-mapping>
