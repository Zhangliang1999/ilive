<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveUserMeetRole" table="ilive_user_meet_role">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="roomId" column="room_id" type="java.lang.Integer"></property>
		<property name="userId" column="user_id" type="java.lang.String"></property>
		<property name="createTime" type="java.sql.Timestamp">
			<column name="create_time" />
		</property>
		<property name="roleType" column="role_type" type="java.lang.Integer"></property>
		<property name="nickName" column="nick_name" type="java.lang.String"></property>
	</class>
</hibernate-mapping>