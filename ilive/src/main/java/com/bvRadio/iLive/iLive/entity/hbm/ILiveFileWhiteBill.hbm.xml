<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveFileWhiteBill" table="ilive_file_whitebill">
		<meta attribute="sync-DAO">false</meta>
		<id name="billId" type="java.lang.Long">
			<column name="bill_id" />
			<generator class="assigned" />
		</id>
		<property name="fileId" type="java.lang.Long">
			<column name="file_id" />
		</property>

		<property name="phoneNum" type="java.lang.String">
			<column name="phone_num" />
		</property>
		<property name="userName" type="java.lang.String">
			<column name="user_name"/>
		</property>
		<property name="exportTime" column="export_time"></property>

	</class>
</hibernate-mapping>
