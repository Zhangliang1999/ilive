		</id>

		<property name="lotteryId">
			<column name="lottery_id"></column>
		</property>

		<property name="userId">
			<column name="user_id"></column>
		</property>

		<property name="userName">
			<column name="user_name"></column>
		</property>

		<property name="phone">
			<column name="phone"></column>
		</property>

		<property name="isPrize">
			<column name="is_prize"></column>
		</property>

		<property name="prize">
			<column name="prize"></column>
		</property>

		<property name="prizepro">
			<column name="prizepro"></column>
		</property>

		<property name="terminal">
			<column name="terminal"></column>
		</property>

		<property name="IP">
			<column name="ip"></column>
		</property>

		<property name="area">
			<column name="area"></column>
		</property>

		<property name="createTime">
			<column name="create_time"></column>
		</property>

		<property name="prizeId">
			<column name="prize_id"></column>
		</property>

	</class>
</hibernate-mapping>