<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveMeetRequest" table="ilive_meet_request">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="java.lang.String">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="roomId" column="room_id" type="java.lang.Integer"></property>
		<property name="type" column="type" type="java.lang.Integer"></property>
		<property name="password" column="password" type="java.lang.String"></property>
		<property name="pic" column="pic" type="java.lang.String"></property>
		<property name="loginUrl" column="login_url" type="java.lang.String"></property>
		<property name="message" column="message" type="java.lang.String"></property>
		<property name="roomName" column="room_name" type="java.lang.String"></property>
	</class>
</hibernate-mapping>