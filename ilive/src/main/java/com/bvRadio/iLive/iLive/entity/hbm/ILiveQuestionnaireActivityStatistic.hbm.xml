<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveQuestionnaireActivityStatistic" table="ilive_questionnaire_activity_statistic">

		<meta attribute="sync-DAO">false</meta>

		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>

		<property name="questionnaireId">
			<column name="questionnaire_id"></column>
		</property>

		<property name="startTime">
			<column name="start_time"></column>
		</property>

		<property name="endTime">
			<column name="end_time"></column>
		</property>

		<property name="userId">
			<column name="user_id"></column>
		</property>
		
		
	</class>
</hibernate-mapping>