<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveEstoppel" table="ilive_estoppel">
		<meta attribute="sync-DAO">false</meta>
		<composite-id>
			<key-property name="roomId" column="room_id" type="java.lang.Integer" />
			<key-property name="userId" column="user_id" type="java.lang.Long" />
		</composite-id>
		<property name="userIp" column="user_ip"></property>
		<property name="createTime" column="create_time"></property>
	</class>
</hibernate-mapping>
