<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveFieldIdManager" table="ilive_field_id_manager">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="tableName" type="java.lang.String">
			<column name="table_name" not-null="true" />
		</property>
		<property name="fieldName" type="java.lang.String">
			<column name="field_name" not-null="true" />
		</property>
		<property name="nextId" type="java.lang.Long">
			<column name="next_id" />
		</property>
		<property name="maxId" type="java.lang.Integer">
			<column name="max_id" />
		</property>
	</class>
</hibernate-mapping>
