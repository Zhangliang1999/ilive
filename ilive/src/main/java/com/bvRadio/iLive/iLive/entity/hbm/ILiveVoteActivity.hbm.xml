<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="ILiveVoteActivity" table="ilive_vote_activity">

		<meta attribute="sync-DAO">false</meta>

		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>

		<property name="roomId">
			<column name="room_id"></column>
		</property>

		<property name="voteName">
			<column name="vote_name"></column>
		</property>

		<property name="startTime">
			<column name="start_time"></column>
		</property>

		<property name="endTime">
			<column name="end_time"></column>
		</property>

		<property name="isOpen">
			<column name="is_open"></column>
		</property>

		<property name="number">
			<column name="number"></column>
		</property>


		<property name="isEnd">
			<column name="is_end"></column>
		</property>

		<property name="isBeforeSwitch">
			<column name="is_before_switch"></column>
		</property>

		<property name="createTime">
			<column name="create_time"></column>
		</property>

		<property name="updateTime">
			<column name="update_time"></column>
		</property>

		<property name="enterpriseId">
			<column name="enterprise_id"></column>
		</property>

		<property name="userId">
			<column name="user_id"></column>
		</property>

	</class>
</hibernate-mapping>