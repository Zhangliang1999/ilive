<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bvRadio.iLive.iLive.entity">
	<class name="AutoLogin" table="auto_login">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="appId" type="java.lang.String">
			<column name="app_id" />
		</property>
		<property name="secret" type="java.lang.String">
			<column name="secret" />
		</property>
		<property name="nowTime" type="java.lang.String">
			<column name="now_time" />
		</property>

		<property name="token" type="java.lang.String">
			<column name="token" />
		</property>

		<property name="roomId" type="java.lang.Integer">
			<column name="room_id" />
		</property>

		<property name="fieldId" type="java.lang.Integer">
			<column name="field_id" />
		</property>
		
		<property name="isDel" type="java.lang.Integer">
			<column name="is_del" />
		</property>

	</class>
</hibernate-mapping>