<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.jwzt.billing.entity">
	<class name="SettlementLogAndRevenueFlow" table="billing_settlement_log_and_revenue_flow">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="java.lang.String">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="settlementLogId" type="java.lang.Long">
			<column name="settlement_log_id" />
		</property>
		<property name="revenueFlowId" type="java.lang.Long">
			<column name="revenue_flow_id" />
		</property>
		<property name="createTime" type="java.sql.Timestamp">
			<column name="create_time" />
		</property>
	</class>
</hibernate-mapping>
